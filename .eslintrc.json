{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": [0, "windows"],
        "react/destructuring-assignment": "off",
        "react/prop-types": "off",
        "function-paren-newLine": "off",
        "implicit-arrow-linebreak": "off"
    }
}
